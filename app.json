{
  "expo": {
    "name": "Bioler Plate",
    "slug": "bioler-plate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#091B4E"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.umairmanzoor1145.biolerplate",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#091B4E"
      },
      "package": "com.umairmanzoor1145.biolerplate",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "NOTIFICATIONS",
        "POST_NOTIFICATIONS",
        "VIBRATE",
        "RECEIVE_BOOT_COMPLETED",
        "SCHEDULE_EXACT_ALARM"
      ],
      "useNextNotificationsApi": true,
      "targetSdkVersion": 33
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#ffffff",
          "androidMode": "default",
          "androidCollapsedTitle": "New Notification",
          "androidImportance": "max",
          "androidShowBadge": true,
          "androidAllowForegroundService": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "7cc0ad45-e962-46c6-90c9-f31b52392703"
      }
    },
    "scheme": "bioler-plate"
  }
}
